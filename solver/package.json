{
  "name": "borderbound-solver",
  "version": "1.0.0",
  "description": "Node.js port of BorderBound solvers",
  "main": "main.js",
  "type": "commonjs",
  "scripts": {
    "easy": "node --max-old-space-size=8192 main.js ../levelsEasy.xml",
    "medium": "node --max-old-space-size=8192 main.js ../levelsMedium.xml",
    "hard": "node --max-old-space-size=8192 main.js ../levelsHard.xml",
    "community": "node --max-old-space-size=8192 main.js ../levelsCommunity.xml",
    "all": "npm run easy & npm run medium & npm run hard & npm run community"
  },
  "keywords": [
    "puzzle",
    "solver",
    "borderbound"
  ],
  "author": "Wayne",
  "license": "GPL-3.0-only",
  "dependencies": {
    "fastpriorityqueue": "^0.7.5"
  }
}
